{
    "host": {
        "keep_alive_mode": true,
        "keep_alive_interval": 300,
        "header_length": 2,
        "header_length_exists": true
    },
    "terminal": {
        "process_default_dump": true,
        "connect_on_startup": true
    },
    "debug": {
        "level": "INFO",
        "clear_log": true,
        "parse_subfields": false
    },
    "fields": {
        "max_amount": 100,
        "max_amount_limited": true,
        "build_fld_90": true,
        "send_internal_id": true,
        "validation": true,
        "json_mode": true,
        "hide_secrets": true
    },
    "remote_spec": {
        "use_remote_spec": false,
        "rewrite_local_spec": true,
        "remote_spec_url": "",
        "backup_storage_depth": 10,
        "backup_storage":  true
    }
}